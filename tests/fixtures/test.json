            "optional_params" : [
                "trim_user",
                "include_entities"
            ],
            "required_params" : [
                "format"
            ],
            "path" : "/statuses/public_timeline.:format",
            "method" : "get",
            "authentication": true
        },
        "public_timeline2" : {
            "optional_params" : [
                "trim_user",
                "include_entities"
            ],
            "required_params" : [
                "format"
            ],
            "base_url" : "https://api2.twitter.com/2",
            "path" : "/statuses/public_timeline.:format",
            "method" : "GET",
            "authentication": true
        },
        "update_user" : {
            "required_params" : [
                "id"
            ],
