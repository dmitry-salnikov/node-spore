            "required_payload": true
        },
        "reinit_user" : {
            "required_params" : [
                "id"
            ],
            "path" : "/user/:id",
            "method" : "PUT",
            "authentication": true
        },
        "version" : {
            "path" : "/version",
            "method" : "HEAD"
        },
        "version" : {
            "path" : "/version",
            "method" : "HEAD"
        },
        "echo" : {
            "path" : "/echo",
            "method" : "POST"
        },
        "big_method" : {
            "required_params" : [
                "id",
                "format"
            ],
            "path" : "/user/:id.:format",
            "method" : "GET"
        },
        "with_headers" : {
            "headers" : {
                "Accept" : "text/html"
            },
            "path" : "/user/",
            "method" : "GET"
        },
        "with_headers_placeholder" : {
            "required_params": ["language1", "language2"],
