{
    "base_url" : "http://api.twitter.com/1",
    "version"  : "0.1",
    "authentication" : true,
    "formats" : ["json", "html"],
    "methods"  : {
        "public_timeline" : {
            "path" : "/statuses/public_timeline",
            "method" : "GET",
            "authentication": false,
            "formats" : ["xml"]
        },
        "update_user" : {
            "path" : "/user/:id",
            "method" : "POST"
        }
    }
}
